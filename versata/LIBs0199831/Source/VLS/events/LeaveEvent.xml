		</Constraint>
		<Constraint
			Name="Member 2"
			ConditionType="AcceptWhen"
			ErrorAttribute="FKMember"
			>
			<Condition><![CDATA[getMember().getCurrentReservations()=0]]></Condition>
			<ErrorMessage><![CDATA[Member still has pending reservations]]></ErrorMessage>
		</Constraint>
		<Constraint
			Name="Member 3"
			ConditionType="AcceptWhen"
			ErrorAttribute="FKMember"
			>
			<Condition><![CDATA[getMember().getCurrentLoans()=0]]></Condition>
			<ErrorMessage><![CDATA[Member still has pending loans]]></ErrorMessage>
		</Constraint>
