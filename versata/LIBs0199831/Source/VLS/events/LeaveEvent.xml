				DerivationType="None"
			/>
			<Validation
				ValidationType="Condition"
			/>
		</Attribute>
		<Attribute
			Name="FKMember"
			PreventUserUpdates="True"
			ValueRequired="False"
			LayoutByDefault="True"
			Persistent="True"
			>
			<DataType
				DataType="Long"
			/>
			<Derivation
				DerivationType="None"
			/>
			<Validation
				ValidationType="Condition"
			/>
		</Attribute>
		<Attribute
			Name="TimeStamp"
			PreventUserUpdates="False"
			ValueRequired="False"
